service vendor.health-default /system/bin/android.hardware.health-service.qti_recovery
    class hal
    user root
    group root
    capabilities WAKE_ALARM BLOCK_SUSPEND
    file /dev/kmsg w
    setenv LD_LIBRARY_PATH /vendor/lib64:/vendor/lib64/hw:/system/lib64:/sbin
    seclabel u:r:recovery:s0

service vendor.charger /system/bin/android.hardware.health-service.qti_recovery --charger
    class charger
    seclabel u:r:charger_vendor:s0
    user root
    group root wakelock input graphics
    capabilities SYS_BOOT WAKE_ALARM BLOCK_SUSPEND
    file /dev/kmsg w
    setenv LD_LIBRARY_PATH /vendor/lib64:/vendor/lib64/hw:/system/lib64:/sbin
    seclabel u:r:recovery:s0
