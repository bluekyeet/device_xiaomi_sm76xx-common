service vendor.boot-qti /system/bin/android.hardware.boot-service.qti
    class early_hal
    user root
    group root
    setenv LD_LIBRARY_PATH /vendor/lib64:/vendor/lib64/hw:/system/lib64:/sbin
    seclabel u:r:recovery:s0
