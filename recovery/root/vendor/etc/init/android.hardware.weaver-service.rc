on property:vendor.qseecomd.ready=1
    start vendor.weaver-service

service vendor.weaver-service /vendor/bin/hw/android.hardware.weaver-service.thales
    class hal
    user root
    group root drmrpc
    setenv LD_LIBRARY_PATH /vendor/lib64:/vendor/lib64/hw:/odm/lib64:/system/lib64:/sbin
    seclabel u:r:recovery:s0
    disabled
