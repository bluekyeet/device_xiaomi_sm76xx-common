on init
    start vendor.qseecomd

on property:init.svc.vendor.qseecomd=running
    setprop vendor.qseecomd.ready 1

service vendor.qseecomd /vendor/bin/qseecomd
    socket notify-topology stream 660 system drmrpc
    class core
    user root
    group root drmrpc
    setenv LD_LIBRARY_PATH /vendor/lib64:/vendor/lib64/hw:/odm/lib64:/system/lib64:/sbin
    seclabel u:r:recovery:s0
