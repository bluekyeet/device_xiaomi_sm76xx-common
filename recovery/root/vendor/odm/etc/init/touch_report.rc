on charger
    write /sys/class/touch/touch_dev/touch_doze_analysis 6

service odm.touch_report /vendor/odm/bin/touch_report_debug
    class hal
    user root
    group root uhid input
    capabilities SYS_NICE
    ioprio rt 4
    setenv LD_LIBRARY_PATH /vendor/lib64:/vendor/lib64/hw:/vendor/odm/lib64:/system/lib64:/sbin
    disabled
    seclabel u:r:recovery:s0
